/*Zona hacia adelante*/
zone "miempresa.com" {
	type master;
	file "/etc/bind/db.miempresa.com";
};

/*Zona inversa*/
zone "50.168.192.in-addr.arpa" {
	type master;
	file "/etc/bind/db.192";
};
	
